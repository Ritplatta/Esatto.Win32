<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Esatto.Win32</RootNamespace>
    <TargetFrameworks>net48;net7.0</TargetFrameworks>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <DefineConstants>$(DefineConstants);ESATTO_WIN32;ESATTO_WIN32_COMMON</DefineConstants>
    <Title>Esatto Win32 Common</Title>
    <Description>Tools for manipulating processes, services, printers, etc...</Description>
    <PackageReadmeFile>.\Readme.md</PackageReadmeFile>
  </PropertyGroup>
  <Import Project="..\assets\EsattoCSCommon.targets" />
  <ItemGroup>
    <PackageReference Include="System.Reactive.Linq" Version="5.0.0" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Readme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>